<% @stations.each do |station| %>
  <div class="span8">
  <h4> <a href="/stations/<%=station.id%>"> <%= station.name %> </a></h4>
  <ul>
    <li>Id: <%= station.id %> </li>
    <li>Latitude: <%= station.latitude %> </li>
    <li>Longitude: <%= station.longitude %> </li>
    <li>Bikes available: <%= station.availableBikes %> </li>
    <li>Docks available : <%= station.availableDocks %> </li> 
  </ul>
</div>
<% end %>
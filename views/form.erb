<h1> Pick start and end </h1>
<br>
<form action='/map' method="POST">
  <select name="start"> 
    <% @data.each do |entry| %>
      <option value="<%=[entry["lat"].to_f/1000000.0, entry["lng"].to_f/1000000.0]%>"><%=entry["name"]%></option>
    <% end %>
  </select> <br>
  <select name="end"> 
    <% @data.each do |entry| %>
      <option value="<%=[entry["lat"].to_f/1000000.0, entry["lng"].to_f/1000000.0]%>"> <%=entry["name"]%></option>
    <% end %>
  </select> <br>
  <input type="submit" value="submit"/>
</form>